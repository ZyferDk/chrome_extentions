{
    "name": "Kelas Extension",
    "description": "Make extension chrome",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "Assets/img/favicon/favicon-16x16.png",
            "32": "Assets/img/favicon/favicon-32x32.png",
            "192": "Assets/img/favicon/android-chrome-192x192.png",
            "512": "Assets/img/favicon/android-chrome-512x512.png"
        }
    },
    "icons": {
        "16": "Assets/img/favicon/favicon-16x16.png",
        "32": "Assets/img/favicon/favicon-32x32.png",
        "192": "Assets/img/favicon/android-chrome-192x192.png",
        "512": "Assets/img/favicon/android-chrome-512x512.png"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    }
}